{
    "name": "lbhurtado/form-flow-manager",
    "description": "Driver-based form flow orchestration system with DirXML-style mapping",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Lester Hurtado",
            "email": "lester@hurtado.ph"
        }
    ],
    "autoload": {
        "psr-4": {
            "LBHurtado\\FormFlowManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LBHurtado\\FormFlowManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LBHurtado\\FormFlowManager\\FormFlowServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/yaml": "^7.4",
        "spatie/laravel-data": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.3",
        "pestphp/pest": "^3.8"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
