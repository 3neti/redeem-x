{
    "name": "lbhurtado/merchant",
    "description": "Merchant and Vendor Alias Registry",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Lester Hurtado",
            "email": "lester@hurtado.ph"
        }
    ],
    "autoload": {
        "psr-4": {
            "LBHurtado\\Merchant\\": "src/",
            "LBHurtado\\Merchant\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LBHurtado\\Merchant\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "spatie/laravel-data": "^4.15",
        "lorisleiva/laravel-actions": "^2.9"
    },
    "require-dev": {
        "orchestra/testbench": "^10.3",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LBHurtado\\Merchant\\MerchantServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
